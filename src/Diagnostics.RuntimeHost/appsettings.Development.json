{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "CompilerHost": {
    "CompilerHostBinaryLocation": "..\\Diagnostics.CompilerHost\\bin\\Debug\\netcoreapp2.0",
    "CompilerHostPort": "7000",
    "PollingIntervalInSeconds": "60",
    "ProcessMemoryThresholdInMB": "300"
  },
  "AzureSupportCenter": {
    "ApiUri": "/api/diagnosis/",
    "ApiVersion": "2018-02-01",
    "UserAgent": "AppLensClient"
  },
  "SourceWatcher": {
    "WatcherType": "1",
    "PollingIntervalInSeconds": "10",
    "Local": {
      "LocalScriptsPath": ""
    },
    "Github": {
      "DestinationScriptsPath": "C:\\Detectors",
      "RepoName": "detectorslist",
      "Branch": "master"
    }
  },
  "Kusto": {
    "DBName": "wawsprod",
    "KustoRegionGroupings": "BAY:MWH:MSFTBAY,BLU:BN1:YQ1:MSFTBLU,CH1:CQ1:CY4:DM1:SN1:YT1,AM2,DB3:MSFTDB3,*",
    "KustoClusterNameGroupings": "wawswusfollower,wawseusfollower,wawscusfollower,wawsweufollower,wawsneufollower,wawseasfollower",
    "KustoClusterFailoverGroupings": "wawswus,wawseus,wawscus,wawsweu,wawsneu,wawseas",
    "HeartBeatConsecutiveFailureLimit": 5,
    "HeartBeatQuery": "RoleInstanceHeartbeat | where TIMESTAMP >= ago(30m) | take 1",
    "HeartBeatTimeOutInSeconds": 10,
    "HeartBeatDelayInSeconds": 20
  },
  "GeoMaster": {
    "GeoCertThumbprint": "",
    "GeoEndpointAddress": "",
    "Token": ""
  },
  "AppInsights": {
  },
  "Mdm": {
    "CertificateThumbprint": "",
    "MonitoringAccount": ""
  },
  "ChangeAnalysis": {
    "Apiversion": "2019-04-01-preview"
  }
}